# Discord Bot Token (Discord Developer Portal'dan alın)
DISCORD_TOKEN=your_bot_token_here

# Sunucu ID (Discord'da Developer Mode açıp sunucuya sağ tık > Copy Server ID)
GUILD_ID=your_guild_id_here

# Rate limit ayarları (milisaniye) - Her unban arasındaki gecikme
# Minimum 500ms önerilir, Discord API limitlerini aşmamak için
UNBAN_DELAY=1000

# Log ayarları - Detaylı log kaydı için true yapın
ENABLE_LOGGING=true

# İsteğe bağlı ayarlar:
# MAX_CONCURRENT_UNBANS=1  # Aynı anda işlenecek maksimum unban sayısı
# RETRY_ATTEMPTS=3         # Başarısız unban denemeleri için yeniden deneme sayısı
